<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gbpiet">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".collegeAppclasss"
        android:allowBackup="true"
        android:icon="@drawable/ic_main_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GBPIET">
        <activity android:name=".drawer.drawer_headerActivity"></activity>
        <activity android:name=".DeveloperActivity" />
        <activity android:name=".splashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".authentication.ForgetPassword" />
        <activity android:name=".authentication.LoginActivity" />
        <activity android:name=".authentication.RegisterActivity" />
        <activity android:name=".fullImageView" />
        <activity
            android:name=".ui.Ebook.EbookActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>